<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".Activities.MainTestActivity">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/alabaster">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="0dp"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/alabaster"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

        <TextView
            android:id="@+id/parkingmapTextview2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="24dp"
            android:fontFamily="@font/clashgrotesk_regular"
            android:text="Settings"
            android:textColor="@color/black"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintStart_toStartOf="@+id/toolbar"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="105dp"
            android:layout_height="35dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/toolbar"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/img"
            tools:ignore="MissingConstraints" />

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/settingsprofilebutton"
                style="@style/Widget.Material3.Button.IconButton"
                android:layout_width="160dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:layout_marginStart="35dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/clashgrotesk_regular"
                android:text="My Profile"
                android:textColor="@color/carolineblue"
                android:textSize="16sp"
                android:textStyle="bold"
                app:backgroundTint="@color/prussianbluek"
                app:cornerRadius="10dp"
                app:elevation="100dp"
                app:icon="@drawable/personicon"
                app:iconGravity="textEnd"
                app:iconPadding="16dp"
                app:iconSize="30dp"
                app:iconTint="@null"
                app:layout_constraintEnd_toStartOf="@id/googleButton1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/continueWithText1"
                app:strokeColor="@color/white" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/settingsparkinghistory"
                style="@style/Widget.Material3.Button.IconButton"
                android:layout_width="160dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:layout_marginStart="35dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/clashgrotesk_regular"
                android:text="Parking History"
                android:textColor="@color/carolineblue"
                android:textSize="16sp"
                android:textStyle="bold"
                app:backgroundTint="@color/prussianbluek"
                app:cornerRadius="10dp"
                app:elevation="100dp"
                app:icon="@drawable/receipticon"
                app:iconGravity="textEnd"
                app:iconPadding="16dp"
                app:iconSize="30dp"
                app:iconTint="@null"
                app:layout_constraintEnd_toStartOf="@id/googleButton1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/continueWithText1"
                app:strokeColor="@color/white" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/settingsvehiclebutton"
                style="@style/Widget.Material3.Button.IconButton"
                android:layout_width="160dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:layout_marginStart="35dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/clashgrotesk_regular"
                android:text="Update Vehicle"
                android:textColor="@color/carolineblue"
                android:textSize="16sp"
                android:textStyle="bold"
                app:backgroundTint="@color/prussianbluek"
                app:cornerRadius="10dp"
                app:icon="@drawable/caricon"
                app:iconGravity="textEnd"
                app:iconPadding="16dp"
                app:iconSize="30dp"
                app:iconTint="@null"
                app:layout_constraintEnd_toStartOf="@id/googleButton1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/continueWithText1"
                app:strokeColor="@color/white" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/settingsAutopay"
                style="@style/Widget.Material3.Button.IconButton"
                android:layout_width="160dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:layout_marginStart="35dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/clashgrotesk_regular"
                android:text="Auto Pay"
                android:textColor="@color/carolineblue"
                android:textSize="16sp"
                android:textStyle="bold"
                app:backgroundTint="@color/prussianbluek"
                app:cornerRadius="10dp"
                app:elevation="100dp"
                app:icon="@drawable/autopayicon"
                app:iconGravity="textEnd"
                app:iconPadding="16dp"
                app:iconSize="30dp"
                app:iconTint="@null"
                app:layout_constraintEnd_toStartOf="@id/googleButton1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/continueWithText1"
                app:strokeColor="@color/white" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/settingspaymenthistory"
                style="@style/Widget.Material3.Button.IconButton"
                android:layout_width="160dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:layout_marginStart="35dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/clashgrotesk_regular"
                android:text="Payment Methods"
                android:textColor="@color/carolineblue"
                android:textSize="16sp"
                android:textStyle="bold"
                app:backgroundTint="@color/prussianbluek"
                app:cornerRadius="10dp"
                app:elevation="100dp"
                app:icon="@drawable/paymenticon"
                app:iconGravity="textEnd"
                app:iconPadding="16dp"
                app:iconSize="30dp"
                app:iconTint="@null"
                app:layout_constraintEnd_toStartOf="@id/googleButton1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/continueWithText1"
                app:strokeColor="@color/white" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/settingsadminbutton"
                style="@style/Widget.Material3.Button.IconButton"
                android:layout_width="160dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:layout_marginStart="35dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/clashgrotesk_regular"
                android:text="Admin"
                android:textColor="@color/carolineblue"
                android:textSize="16sp"
                android:textStyle="bold"
                app:backgroundTint="@color/prussianbluek"
                app:cornerRadius="10dp"
                app:elevation="100dp"
                app:icon="@drawable/adminicon"
                app:iconGravity="textEnd"
                app:iconPadding="16dp"
                app:iconSize="30dp"
                app:iconTint="@null"
                app:layout_constraintEnd_toStartOf="@id/googleButton1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/continueWithText1"
                app:strokeColor="@color/white" />

        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:backgroundTint="@color/alabaster"
        app:headerLayout="@layout/nav_header"
        app:itemTextColor="@color/black"
        app:menu="@menu/drawer_menu" />

</androidx.drawerlayout.widget.DrawerLayout>

